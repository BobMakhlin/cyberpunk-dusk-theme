{
  "$schema": "vscode://schemas/color-theme",
  "name": "Cyberpunk Dusk",
  "type": "dark",
  "semanticHighlighting": true,

  "semanticTokenColors": {
    // Types / structure
    "type": "#9A8CC2",
    "class": "#9A8CC2",
    "interface": "#A39AD1",
    "enum": "#A39AD1",
    "struct": "#9A8CC2",
    "typeParameter": "#A39AD1",

    // Functions / methods
    "function": "#7898BE",
    "method": "#7898BE",

    // Properties / fields — key distinction from locals
    "property": "#79AFAE",
    "property.readonly": "#7FB8B7",
    "property.static": "#6C9B9B",

    // Variables — calmer, so fields stand out
    "variable": "#9AA6B2",
    "variable.readonly": "#A3ACB8",
    "variable.readonly.defaultLibrary": "#A08FB4",
    "parameter": "#8F9BA9",

    // Enums / annotations
    "enumMember": "#B5966B",
    "decorator": "#B5966B",
    "annotation": "#B5966B",

    // Namespaces / modules
    "namespace": "#74A1B0",
    "module": "#74A1B0",

    // Keywords / modifiers (if semantic token available)
    "keyword": "#5B9C9E",
    "modifier": "#5B9C9E",

    // Macros / preprocessor
    "macro": "#B5966B",

    // Strings / numbers / regexp
    "string": "#B4986F",
    "number": "#AE826B",
    "regexp": "#B4986F",

    // Comments
    "comment": { "foreground": "#6C7688", "fontStyle": "italic" }
  },

  "tokenColors": [
    // =============================================
    // COMMENTS
    // =============================================
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6C7688",
        "fontStyle": "italic"
      }
    },

    // =============================================
    // STRINGS
    // =============================================
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.quoted.double",
        "string.quoted.single",
        "string.template",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#B4986F"
      }
    },
    {
      "name": "String escape sequences",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#B4986F"
      }
    },

    // =============================================
    // NUMBERS & CONSTANTS
    // =============================================
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.numeric.hex",
        "constant.numeric.octal",
        "constant.numeric.binary"
      ],
      "settings": {
        "foreground": "#AE826B"
      }
    },
    {
      "name": "Language constants (boolean, null, etc.)",
      "scope": [
        "constant.language",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#AE826B"
      }
    },
    {
      "name": "Other constants",
      "scope": [
        "constant.other",
        "constant.character",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#A08FB4"
      }
    },

    // =============================================
    // KEYWORDS & STORAGE
    // =============================================
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#5B9C9E"
      }
    },
    {
      "name": "Control flow keywords",
      "scope": [
        "keyword.control",
        "keyword.control.flow",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.control.trycatch",
        "keyword.control.exception"
      ],
      "settings": {
        "foreground": "#6193A8"
      }
    },
    {
      "name": "Storage types and modifiers",
      "scope": [
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#5B9C9E"
      }
    },
    {
      "name": "Primitive types",
      "scope": [
        "storage.type.primitive",
        "support.type.primitive",
        "storage.type.built-in",
        "support.type.builtin"
      ],
      "settings": {
        "foreground": "#A39AD1"
      }
    },

    // =============================================
    // TYPES & CLASSES
    // =============================================
    {
      "name": "Types and classes",
      "scope": [
        "entity.name.type",
        "entity.name.type.class",
        "entity.name.class",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#9A8CC2"
      }
    },
    {
      "name": "Interfaces and type aliases",
      "scope": [
        "entity.name.type.interface",
        "entity.name.type.alias",
        "entity.name.type.enum",
        "entity.name.type.type-parameter"
      ],
      "settings": {
        "foreground": "#A39AD1"
      }
    },
    {
      "name": "Inherited classes",
      "scope": [
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#9A8CC2",
        "fontStyle": "italic"
      }
    },

    // =============================================
    // FUNCTIONS & METHODS
    // =============================================
    {
      "name": "Functions and methods",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#7898BE"
      }
    },
    {
      "name": "Function parameters in definition",
      "scope": [
        "variable.parameter",
        "meta.parameter"
      ],
      "settings": {
        "foreground": "#8F9BA9"
      }
    },

    // =============================================
    // VARIABLES & PROPERTIES
    // =============================================
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#9AA6B2"
      }
    },
    {
      "name": "Properties and fields",
      "scope": [
        "variable.other.property",
        "variable.other.field",
        "variable.other.field.java",
        "variable.other.member",
        "variable.other.member.java",
        "variable.other.object.property",
        "variable.other.property.java",
        "support.type.property-name",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#6C9B9B"
      }
    },
    {
      "name": "Object keys (JSON, YAML, etc.)",
      "scope": [
        "support.type.property-name.json",
        "entity.name.tag.yaml",
        "punctuation.support.type.property-name"
      ],
      "settings": {
        "foreground": "#79AFAE"
      }
    },

    // =============================================
    // ANNOTATIONS / DECORATORS
    // =============================================
    {
      "name": "Annotations and decorators",
      "scope": [
        "meta.annotation",
        "storage.type.annotation",
        "punctuation.definition.annotation",
        "meta.decorator",
        "entity.name.function.decorator",
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#B5966B",
        "fontStyle": "italic"
      }
    },

    // =============================================
    // IMPORTS & NAMESPACES
    // =============================================
    {
      "name": "Imports and modules",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as",
        "meta.import"
      ],
      "settings": {
        "foreground": "#74A1B0"
      }
    },
    {
      "name": "Namespaces and packages",
      "scope": [
        "entity.name.namespace",
        "entity.name.package",
        "entity.name.module",
        "support.other.namespace"
      ],
      "settings": {
        "foreground": "#74A1B0"
      }
    },

    // =============================================
    // OPERATORS & PUNCTUATION
    // =============================================
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.comparison",
        "keyword.operator.ternary"
      ],
      "settings": {
        "foreground": "#8FA4B2"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#9BA6B2"
      }
    },
    {
      "name": "Brackets",
      "scope": [
        "punctuation.bracket",
        "punctuation.section",
        "punctuation.definition.block",
        "punctuation.definition.parameters"
      ],
      "settings": {
        "foreground": "#9BA6B2"
      }
    },

    // =============================================
    // HTML / XML / JSX / MARKUP
    // =============================================
    {
      "name": "HTML/XML tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#5B9C9E"
      }
    },
    {
      "name": "HTML/XML attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#79AFAE"
      }
    },
    {
      "name": "JSX component tags",
      "scope": [
        "support.class.component",
        "entity.name.tag.component"
      ],
      "settings": {
        "foreground": "#9A8CC2"
      }
    },

    // =============================================
    // CSS / SCSS / LESS
    // =============================================
    {
      "name": "CSS selectors",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#9A8CC2"
      }
    },
    {
      "name": "CSS property names",
      "scope": [
        "support.type.property-name.css",
        "meta.property-name.css"
      ],
      "settings": {
        "foreground": "#79AFAE"
      }
    },
    {
      "name": "CSS property values",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#B4986F"
      }
    },
    {
      "name": "CSS units",
      "scope": [
        "keyword.other.unit.css",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#AE826B"
      }
    },
    {
      "name": "CSS pseudo-classes/elements",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#A39AD1"
      }
    },
    {
      "name": "SCSS/Less variables",
      "scope": [
        "variable.scss",
        "variable.less",
        "variable.css"
      ],
      "settings": {
        "foreground": "#79AFAE"
      }
    },

    // =============================================
    // MARKDOWN
    // =============================================
    {
      "name": "Markdown headings",
      "scope": [
        "heading.1.markdown",
        "heading.2.markdown",
        "heading.3.markdown",
        "heading.4.markdown",
        "heading.5.markdown",
        "heading.6.markdown",
        "markup.heading"
      ],
      "settings": {
        "foreground": "#9A8CC2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown links",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#74A1B0"
      }
    },
    {
      "name": "Markdown code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#B4986F"
      }
    },
    {
      "name": "Markdown quotes",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#6C7688",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown list punctuation",
      "scope": [
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#5B9C9E"
      }
    },

    // =============================================
    // SHELL / BASH
    // =============================================
    {
      "name": "Shell commands",
      "scope": [
        "support.function.builtin.shell",
        "entity.name.command.shell"
      ],
      "settings": {
        "foreground": "#7898BE"
      }
    },
    {
      "name": "Shell variables",
      "scope": [
        "variable.other.normal.shell",
        "variable.other.positional.shell",
        "variable.other.special.shell",
        "punctuation.definition.variable.shell"
      ],
      "settings": {
        "foreground": "#79AFAE"
      }
    },

    // =============================================
    // REGEX
    // =============================================
    {
      "name": "Regex groups and quantifiers",
      "scope": [
        "keyword.operator.quantifier.regexp",
        "keyword.operator.or.regexp",
        "keyword.control.anchor.regexp"
      ],
      "settings": {
        "foreground": "#AE826B"
      }
    },
    {
      "name": "Regex character classes",
      "scope": [
        "constant.other.character-class.regexp",
        "constant.character.character-class.regexp"
      ],
      "settings": {
        "foreground": "#A39AD1"
      }
    },

    // =============================================
    // DIFF (using brandbook palette for readability)
    // =============================================
    {
      "name": "Diff inserted",
      "scope": [
        "markup.inserted",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "foreground": "#79AFAE"
      }
    },
    {
      "name": "Diff deleted",
      "scope": [
        "markup.deleted",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "foreground": "#AE826B"
      }
    },
    {
      "name": "Diff changed",
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "foreground": "#74A1B0"
      }
    },

    // =============================================
    // SQL
    // =============================================
    {
      "name": "SQL keywords",
      "scope": [
        "keyword.other.DML.sql",
        "keyword.other.DDL.sql",
        "keyword.other.sql"
      ],
      "settings": {
        "foreground": "#5B9C9E"
      }
    },

    // =============================================
    // INVALID / DEPRECATED
    // =============================================
    {
      "name": "Invalid",
      "scope": [
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#AE826B"
      }
    },
    {
      "name": "Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#8B95A7",
        "fontStyle": "strikethrough"
      }
    }
  ],

